---
# renovate: datasource=github-releases depName=kubernetes/kubernetes
clusterapi_cluster_kubernetes_version: 'v1.32.5'
clusterapi_cluster_openstack_auth_url: ""
clusterapi_cluster_openstack_application_credential_id: ""
clusterapi_cluster_openstack_application_credential_secret: ""
clusterapi_cluster_openstack_domain_name: ""
clusterapi_cluster_openstack_region_name: ""
clusterapi_cluster_openstack_use_existing_image: true

keycloak_install_helm_values_admin_password: supersecretpassword
# Set this to the IP that was shown to you before continuing
keycloak_install_helm_values_hostname: keycloak.example.com
keycloak_install_helm_values_postgres_postgres_password: supersecretpassword
keycloak_install_helm_values_postgres_password: supersecretpassword
keycloak_install_admin_user: admin
keycloak_install_admin_password: supersecretpassword
keycloak_install_admin_email: operations@example.com
# Create this before executing the role. E.g. create a zone in Designate.
keycloak_install_yake_url: gardener.example.com
keycloak_install_client_secret: suipersecretpassword

garden_control_install_yake_url: "{{ keycloak_install_yake_url }}"
garden_control_install_keycloak_url: "{{ keycloak_install_helm_values_hostname }}"
garden_control_install_keycloak_client_secret: "{{ keycloak_install_client_secret }}"

garden_control_install_acme_mail: "{{ keycloak_install_admin_email }}"
garden_control_install_admin_mail: admin@gardener.demo

garden_control_install_openstack_auth_url: "{{ clusterapi_cluster_openstack_auth_url }}"
garden_control_install_openstack_project_name: example
garden_control_install_openstack_domain_name: "{{ clusterapi_cluster_openstack_domain_name }}"
garden_control_install_openstack_application_credential_id: "{{ clusterapi_cluster_openstack_application_credential_id }}"
garden_control_install_openstack_application_credential_name: example
garden_control_install_openstack_application_credential_secret: "{{ clusterapi_cluster_openstack_application_credential_secret }}"
